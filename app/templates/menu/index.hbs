<div class="row pt-4 ">
  <div class="col">
    <h2>Menu List</h2>
  </div>
</div>
<div class="row">
  <div class="col">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">
            Week Of
          </th>
          <th scope="col">
            Month
          </th>
          <th scope="col">
            Year
          </th>
          <th scope="col">
            Cost
          </th>
        </tr>
      </thead>
      <tbody>
        {{#each model as |menu|}}
        <tr>
          <td>
            {{link-to menu.weekOf "menu.view" menu.id }}
          </td>
          <td>
            {{menu.month}}
          </td>
          <td>
            {{menu.year}}
          </td>
          <td>
            {{menu.cost}}
          </td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
